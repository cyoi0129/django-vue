lass=\"pb-0 oneLine\"><v-icon class=\"pr-2\">mdi-clock-outline</v-icon>{{article.updated}}<v-icon class=\"pl-6 pr-2\">mdi-folder</v-icon>{{catLists[index]}}</v-card-subtitle>\n          <v-card-text class=\"text--primary\">\n            <div class=\"threeLine my-2\">{{article.description}}</div>\n          </v-card-text>\n        </v-card>\n      </v-hover>\n      </v-col>\n    </v-row>\n  </v-container>\n</template>\n<script>\nexport default {\n  data: () => ({\n    articles: [],\n    catLists: []\n  }),\n  props:['obj','now'],\n  mounted() {\n    this.articles = this.obj.items\n    for (let i = 0; i < this.obj.items.length; i++) {\n      let catName = this.obj.categories.filter(category => category.index === this.obj.items[i].category)[0].name\n      this.catLists.push(catName)\n    }\n  }\n}\n</script>\n<style scoped>\n.v-icon.v-icon {\n  font-size:18px;\n}\n.oneLine, .twoLine, .threeLine {\n  display:-webkit-box;\n  overflow:hidden;\n  -webkit-box-orient: vertical;\n  -moz-box-orient:vertical;\n}\n.oneLine {\n  -webkit-line-clamp:1;\n}\n.twoLine {\n  -webkit-line-clamp:2;\n  height:80px;\n}\n.threeLine {\n  -webkit-line-clamp:3;\n  height:64px;\n}\n.v-card--reveal {\n  align-items: center;\n  bottom: 0;\n  justify-content: center;\n  opacity: .8;\n  position: absolute;\n  width: 100%;\n}\n</style>"]}]}